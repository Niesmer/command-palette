{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "command-palette",
  "version": "0.1.0",
  "identifier": "com.command-palette.app",
  "build": {
    "beforeDevCommand": "pnpm vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "visible": false,
        "label": "window",
        "resizable": false,
        "fullscreen": true,
        "focus": false,
        "decorations": false,
        "closable": false,
        "transparent": true
      }
    ]
  }
}
